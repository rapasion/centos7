#!/bin/bash
script_location="."

if [ ! -d "/export/home/gml_source/logs/backup" ]; then
    mkdir "/export/home/gml_source/logs/backup"
fi 

$script_location"/db_pg_{{ hospital }}_create_table.sh" > "/export/home/gml_source/logs/backup/{{ hospital }}_archive_backup_"`date +20\%y-\%m-\%d_\%H:\%M:\%S`".log"
sleep 5
$script_location"/db_pg_{{ hospital }}_table_dump.sh" > "/export/home/gml_source/logs/backup/{{ hospital }}_archive_dump_"`date +20\%y-\%m-\%d_\%H:\%M:\%S`".log"
